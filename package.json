  "scripts": {
    "dev-server": "ts-node-dev --respawn src/server/server.ts",
    "dev-client": "TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server --watch",
    "dev": "concurrently --kill-others --prefix '[{name}]' --names 'S,C' -c 'bgBlue.bold,bgGreen.bold' 'yarn dev-server' 'yarn dev-client'",
    "type-check-client": "tsc -p ./src/client/tsconfig.json",
    "lint": "eslint --fix 'src/**/*.{ts,tsx}'",
    "run-server": "ts-node dist/server/server.js",
    "build-server": "tsc -b src/server",
    "build-client": "TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack -p"
  },
  "private": true,
  "devDependencies": {
    "concurrently": "^5.2.0",
